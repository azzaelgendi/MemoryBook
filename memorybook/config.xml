<?xml version='1.0' encoding='utf-8'?>
<widget id="io.azza.memoryapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets"
        xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MemoryApp</name>
    <description>
        A memories book app respond to deviceready event.
    </description>
    <author email="aelgendy7114@conestogac.on.ca" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html"/>
    <plugin name="cordova-plugin-whitelist" spec="1"/>
    <access origin="*"/>
    <allow-intent href="http://*/*"/>
    <allow-intent href="https://*/*"/>
    <allow-intent href="tel:*"/>
    <allow-intent href="sms:*"/>
    <allow-intent href="mailto:*"/>
    <allow-intent href="geo:*"/>
    <platform name="android">
        <allow-intent href="market:*"/>
    </platform>
    <icon src="www/img/AE-icon.png"/>
    <splash src="www/img/splash.png"/>
    <feature name="Calendar">
        <param name="android-package" value="nl.xservices.plugins.Calendar"/>
    </feature>
    <plugin name="cordova-plugin-calendar"/>
</widget>
